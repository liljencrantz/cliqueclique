rm ~/.config/cliqueclique/node.db contrib/rootdoc.exp
python manage.py syncdb --noinput
NODE1=$(python manage.py createnode test1 saltgurka)
NODE2=$(python manage.py createnode test2 saltgurka)
ROOTDOC=$(python manage.py postdocument --bookmark $NODE1 contrib/rootdoc)
python manage.py exportdocument $NODE1 $ROOTDOC contrib/rootdoc.exp 
python manage.py importdocument --bookmark $NODE2 contrib/rootdoc.exp 

echo "Node 1: $NODE1"
echo "Node 2: $NODE2"
echo "Root: $ROOTDOC"

python manage.py runrouter
