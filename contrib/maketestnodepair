rm node.db
python manage.py syncdb --noinput
NODE1=$(python manage.py createnode test1 saltgurka)
NODE2=$(python manage.py createnode test2 saltgurka)
ROOTDOC=$(python manage.py postdocument $NODE1 rootdoc)
python manage.py exportdocument $NODE1 $ROOTDOC rootdoc.exp 
python manage.py importdocument $NODE2 rootdoc.exp 
python manage.py runrouter
